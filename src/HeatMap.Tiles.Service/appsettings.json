{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.File",
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": "Verbose",
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "logs/log-.txt",
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "Console"
            }
        ]
    },
    "DB": "/data/work/bikedataproject/config/production.txt",
    "data": "/media/xivk/2T-SSD-EXT/temp-dev",
    "output": "/media/xivk/2T-SSD-EXT/temp-dev/output"
}